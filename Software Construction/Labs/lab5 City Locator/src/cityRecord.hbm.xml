
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class  name="CityRecord" table="cityrecord">
      
      <id name="location_id">  
         <generator class="assigned"></generator>  
        </id>  
                
        <property name="country"></property>  
        <property name="region"></property>  
        
        <property name="city"></property>  
        <property name="postalCode"></property>  
        
        <property name="areaCode"></property>  
        <property name="metroCode"></property>  
        <property name="longitude"></property>  
        <property name="latitude"></property>  

      
  </class>


</hibernate-mapping>
